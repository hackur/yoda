{
  "name": "yoda",
  "productName": "Yoda",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "copy": "cp app/renderer/index.html dist/renderer/index.html",
    "dev-server": "node -r babel-register server.js",
    "dev-browser": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./app/browser/browser.js",
    "dev": "concurrently --kill-others \"npm run dev-server\" \"npm run dev-browser\"",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --colors",
    "build": "npm run clean && npm run build-main && npm run build-renderer && npm run copy",
    "dist": "cross-env NODE_ENV=production electron ./dist/browser/browser.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whoisandie/yoda.git"
  },
  "keywords": [],
  "author": "whoisandie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/whoisandie/yoda/issues"
  },
  "homepage": "https://github.com/whoisandie/yoda#readme",
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "concurrently": "^2.1.0",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "electron-debug": "^1.0.0",
    "electron-packager": "^7.0.2",
    "electron-prebuilt": "^1.2.0",
    "electron-rebuild": "^1.1.5",
    "eslint": "^2.11.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "json-loader": "^0.5.4",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "aphrodite": "^0.4.0",
    "classnames": "^2.2.5",
    "mobx": "^2.3.1",
    "mobx-react": "^3.3.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router": "^2.4.1",
    "source-map-support": "^0.4.0"
  }
}
